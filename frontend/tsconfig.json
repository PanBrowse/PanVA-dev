{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
    },
    "composite": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "checkJs": false,
    "strict": false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,

    "types": [
      "node"
    ],
    "typeRoots": [
      "./types"
    ]
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
